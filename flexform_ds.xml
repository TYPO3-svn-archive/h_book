<T3DataStructure>
    <meta>
       <langDisable>1</langDisable> 
	  <!--  <langChildren>1</langChildren>!-->
        </meta>
  <sheets>
  	<s_general>
		<ROOT>
			<TCEforms>
				<sheetTitle>LLL:EXT:h_book/locallang.xml:flex.general</sheetTitle>
			</TCEforms>
			<type>array</type>
			<el>
				<what_to_display>
					<TCEforms>
						<label>LLL:EXT:h_book/locallang.xml:flex.general.what_to_display</label>
						<config>
							<type>select</type>
							<items type="array">
							<numIndex index="0" type="array">
								<numIndex index="0">LLL:EXT:h_book/locallang.xml:flex.general.what_to_display.display</numIndex>
								<numIndex index="1">DISPLAY</numIndex>
							</numIndex>
							<numIndex index="1" type="array">
								<numIndex index="0">LLL:EXT:h_book/locallang.xml:flex.general.what_to_display.new</numIndex>
								<numIndex index="1">NEW</numIndex>
							</numIndex>
							<numIndex index="2" type="array">
								<numIndex index="0">LLL:EXT:h_book/locallang.xml:flex.general.what_to_display.edit</numIndex>
								<numIndex index="1">EDIT</numIndex>
							</numIndex>
							</items>
							<maxitems>100</maxitems>
							<size>6</size>
							<multiple>1</multiple>
							<selectedListStyle>width:150px</selectedListStyle>
							<itemListStyle>width:150px</itemListStyle>
						</config>
					</TCEforms>
				</what_to_display>
				<limit>
			  		<TCEforms>
						<label>LLL:EXT:h_book/locallang.xml:flex.general.limit</label>
						<config>
							<type>input</type>
							<eval>num</eval>
							<checkbox></checkbox>
							<size>3</size>
						</config>
					</TCEforms>
				</limit>
				<local>
					<TCEforms>
						<label>LLL:EXT:h_book/locallang.xml:flex.general.local</label>
						<config>
							<type>check</type>
						</config>
					</TCEforms>
				</local>
				<storagePID>
					<TCEforms>
						<label>LLL:EXT:h_book/locallang.xml:flex.general.storagePage</label>
						<config>
							<type>group</type>
							<internal_type>db</internal_type>
							<allowed>pages</allowed>
							<size>1</size>
							<maxitems>1</maxitems>
							<minitems>0</minitems>
							<show_thumbs>1</show_thumbs>
						</config>
					</TCEforms>
				</storagePID>
				<editPid>
					<TCEforms>
						<label>LLL:EXT:h_book/locallang.xml:flex.general.editPage</label>
						<config>
							<type>group</type>
							<internal_type>db</internal_type>
							<allowed>pages</allowed>
							<size>1</size>
							<maxitems>1</maxitems>
							<minitems>0</minitems>
							<show_thumbs>1</show_thumbs>
						</config>
					</TCEforms>
				</editPid>
			</el>
		</ROOT>
	</s_general>
	<s_display>
		<ROOT>
			<TCEforms>
				<sheetTitle>LLL:EXT:h_book/locallang.xml:flex.display</sheetTitle>
			</TCEforms>
			<type>array</type>
			<el>
				<orderby>
					<TCEforms>
						<label>LLL:EXT:h_book/locallang.xml:flex.display.orderby</label>
						<config>
							<type>select</type>
							<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="0"></numIndex>
									<numIndex index="1"></numIndex>
								</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="0">LLL:EXT:h_book/locallang.xml:flex.display.orderby.crdate.desc</numIndex>
									<numIndex index="1">crdate DESC</numIndex>
								</numIndex>
								<numIndex index="2" type="array">
									<numIndex index="0">LLL:EXT:h_book/locallang.xml:flex.display.orderby.crdate.asc</numIndex>
									<numIndex index="1">crdate ASC</numIndex>
								</numIndex>
								<numIndex index="3" type="array">
									<numIndex index="0">LLL:EXT:h_book/locallang.xml:flex.display.orderby.author.desc</numIndex>
									<numIndex index="1">author DESC</numIndex>
								</numIndex>
								<numIndex index="4" type="array">
									<numIndex index="0">LLL:EXT:h_book/locallang.xml:flex.display.orderby.author.asc</numIndex>
									<numIndex index="1">author ASC</numIndex>
								</numIndex>
							</items>
							<size>1</size>
						</config>
					</TCEforms>
				</orderby>
				<maxAge>
			  		<TCEforms>
						<label>LLL:EXT:h_book/locallang.xml:flex.display.maxAge</label>
						<config>
							<type>input</type>
							<eval>num</eval>
							<checkbox></checkbox>
							<size>10</size>
						</config>
					</TCEforms>
				</maxAge>
				<maxPageNum>
					<TCEforms>
						<label>LLL:EXT:h_book/locallang.xml:flex.display.pageNum</label>
						<config>
							<type>input</type>
							<eval>num</eval>
							<checkbox>1</checkbox>
							<size>5</size>
						</config>
					</TCEforms>
				</maxPageNum>
			</el>
		</ROOT>
	</s_display>
	<s_create>
		<ROOT>
			<TCEforms>
				<sheetTitle>LLL:EXT:h_book/locallang.xml:flex.create</sheetTitle>
			</TCEforms>
			<type>array</type>
			<el>
				<redirect>
					<TCEforms>
						<label>LLL:EXT:h_book/locallang.xml:flex.create.redirect</label>
						<config>
							<type>group</type>
							<internal_type>db</internal_type>
							<allowed>pages</allowed>
							<size>1</size>
							<maxitems>1</maxitems>
							<minitems>0</minitems>
							<show_thumbs>1</show_thumbs>
						</config>
					</TCEforms>
				</redirect>
				<required>
					<TCEforms>
						<label>LLL:EXT:h_book/locallang.xml:flex.create.required</label>
						<config>
							<type>select</type>
							<items type="array">
							<numIndex index="0" type="array">
								<numIndex index="0">LLL:EXT:h_book/locallang.xml:flex.create.required.name</numIndex>
								<numIndex index="1">name</numIndex>
							</numIndex>
							<numIndex index="1" type="array">
								<numIndex index="0">LLL:EXT:h_book/locallang.xml:flex.create.required.email</numIndex>
								<numIndex index="1">email</numIndex>
							</numIndex>
							<numIndex index="2" type="array">
								<numIndex index="0">LLL:EXT:h_book/locallang.xml:flex.create.required.homepage</numIndex>
								<numIndex index="1">homepage</numIndex>
							</numIndex>
							<numIndex index="3" type="array">
								<numIndex index="0">LLL:EXT:h_book/locallang.xml:flex.create.required.text</numIndex>
								<numIndex index="1">text</numIndex>
							</numIndex>
							<numIndex index="4" type="array">
								<numIndex index="0">LLL:EXT:h_book/locallang.xml:flex.create.required.location</numIndex>
								<numIndex index="1">location</numIndex>
							</numIndex>
							<numIndex index="5" type="array">
								<numIndex index="0">LLL:EXT:h_book/locallang.xml:flex.create.required.icq</numIndex>
								<numIndex index="1">icq</numIndex>
							</numIndex>
							<numIndex index="6" type="array">
								<numIndex index="0">LLL:EXT:h_book/locallang.xml:flex.create.required.msn</numIndex>
								<numIndex index="1">msn</numIndex>
							</numIndex>
							<numIndex index="7" type="array">
								<numIndex index="0">LLL:EXT:h_book/locallang.xml:flex.create.required.aim</numIndex>
								<numIndex index="1">aim</numIndex>
							</numIndex>
							</items>
							<maxitems>100</maxitems>
							<size>6</size>
							<multiple>1</multiple>
							<selectedListStyle>width:150px</selectedListStyle>
							<itemListStyle>width:150px</itemListStyle>
						</config>
					</TCEforms>
				</required>
				<doFilter>
					<TCEforms>
						<label>LLL:EXT:h_book/locallang.xml:flex.create.filter</label>
						<config>
							<type>select</type>
							<items type="array">
								<numIndex index="0" type="array">
									<numIndex index="0"></numIndex>
									<numIndex index="1"></numIndex>
								</numIndex>
								<numIndex index="1" type="array">
									<numIndex index="0">LLL:EXT:h_book/locallang.xml:flex.create.filter.off</numIndex>
									<numIndex index="1">0</numIndex>
								</numIndex>
								<numIndex index="2" type="array">
									<numIndex index="0">LLL:EXT:h_book/locallang.xml:flex.create.filter.on</numIndex>
									<numIndex index="1">1</numIndex>
								</numIndex>                                                
							</items>
							<maxitems>1</maxitems>
							<size>1</size>
						</config>
					</TCEforms>
                </doFilter>
				<filterWords>
					<TCEforms>
						<label>LLL:EXT:h_book/locallang.xml:flex.create.filterWords</label>
						<config>
							<type>text</type>
						</config>
					</TCEforms>
				</filterWords>
			</el>
		</ROOT>
	</s_create>
  </sheets>
</T3DataStructure>